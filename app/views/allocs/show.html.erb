<% provide(:title) %>
<div style="float:left; vertical-align: top">
Download:
  <%= link_to "Excel", alloc_path(format: "xls") %><br /> 
View Allocation Result:
  <%= link_to 'Results', result_path %>
</div>
<div style="float:right; vertical-align: top">
	<ul class='nav nav-pills'>
	<li class="active"><%= link_to 'Back', allocs_path %></li>
	</ul>
</div>
<table class="table table-striped table-bordered table-condensed" align='center' cellpadding='5'>
  <tr>
	<th class='center_content' width='10%'>Student URN</th>
    <th class='center_content'>Student Name</th>
    <th class='center_content' width='5%'>Rank</th>
	<th class='center_content' width='10%'>Project ID</th>
	<th class='center_content'>Selected Projects</th>
	<th class='center_content' width='10%'>Academic URN</th>
	<th class='center_content'>Academic Name</th>
  </tr>
	
  
<% @choices.to_a.each do |s| %>
	<tr>
	<td class='center_content'><%= s.sprofile.nil? ?  "empty" : s.sprofile.urn %></td>
	<td><%= s.sprofile.nil? ?  "empty" : s.sprofile.name%></td>
	<td class='center_content'><%= s.nil? ? 'nil' : s.position %></td>
	<td class='center_content'><%= s.project.nil? ? "empty" : s.project.number %></td>
	<td><%= s.project.nil? ? 'nil' : s.project.title %></td>
	<td class='center_content'><%= s.aprofile.nil? ? "empty" : s.aprofile.urn %></td>
	<td><%= s.aprofile.nil? ? "empty" : s.aprofile.name %></td>
	</tr>
<%end%>
  </table>


